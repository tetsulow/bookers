<h1>Editing Book</h1>

<%= form_with model: @list, url: update_list_path(@list.id), method: :patch, local: true do |f| %>

  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Body</h4>
  <%= f.text_area :body %>

 <br> <%= f.submit 'Update Book' %>

 <p><%= link_to 'Show',  show_list_path %> |
    <%= link_to 'Back' , list_path %>
 </p>

<% end %>

